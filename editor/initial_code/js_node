var assert = require('assert');

if (!global.is_checking) {
    // These "asserts" are used for self-checking
    assert.equal(isAcceptablePassword('short'), false);
    assert.equal(isAcceptablePassword('muchlonger'), false);
    assert.equal(isAcceptablePassword('ashort'), false);
    assert.equal(isAcceptablePassword('muchlonger5'), true);
    assert.equal(isAcceptablePassword('sh5'), false);
    assert.equal(isAcceptablePassword('1234567'), false);

    console.log("Coding complete? Click 'Check' to earn cool rewards!");
}
    